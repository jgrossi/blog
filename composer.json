{
	"repositories": [
		{
			"type": "composer",
			"url": "https://wpackagist.org"
		}
    ],
    "require": {
        "vlucas/phpdotenv": "^2.4",
        "wpackagist-theme/twentyfifteen": "1.9",
        "wpackagist-plugin/akismet": "4.0.3",
        "wpackagist-plugin/enlighter": "3.5",
        "wpackagist-plugin/google-analytics-dashboard-for-wp": "5.3.1.1",
        "wpackagist-plugin/google-sitemap-generator": "4.0.9",
        "wpackagist-plugin/html-editor-syntax-highlighter": "2.2.3",
        "wpackagist-plugin/simple-social-buttons": "2.0.12",
        "wpackagist-plugin/w3-total-cache": "0.9.6",
        "wpackagist-plugin/wp-mail-smtp": "1.2.5"
    },
    "extra": {
		"installer-paths": {
			"./public/wp-content/plugins/{$name}/": ["type:wordpress-plugin"],
			"./public/wp-content/themes/{$name}/": ["type:wordpress-theme"]
		}
	}
}
